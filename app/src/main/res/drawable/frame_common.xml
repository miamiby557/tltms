<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 填充颜色 -->
    <solid android:color="@color/bluenormal"></solid>
    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="1dp"
        android:color="@color/bluenormal"></stroke>
    <!-- 矩形的圆角半径 -->
    <corners android:radius="6px" />
    <padding
        android:bottom="0dp"
        android:left="10dp"
        android:right="10dp"
        android:top="3dp" />
</shape>